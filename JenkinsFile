pipeline{
    agent any
    tools{
        maven "maven"
        jdk "java"
        git "git"
    }
    stages{
        stage('CheckOut from GIT'){
            steps{
                echo "Checking out from git"
                git branch: 'Main' , url: 'https://github.com/Shadhanah-Rajan/PetStoreWebApp.git'
            }
        }
        stage('Clean Package'){
            steps{
                git branch: 'Main' , url: 'https://github.com/devopsusergit/PetStoreWebApp.git'
                echo "Running clean package"
                sh "mvn -Dmaven.test.failure.ignore=true clean package"
            }
        }
        stage('Acceptance Testing'){
            steps{
                echo "Acceptance testing"
            }
        }
        stage('Regression Testing'){
            steps{
                echo "Regression testing"
            }
        }
        parallel{
        stage('Performance Testing'){
            steps{
                echo "Performance testing"
            }
        }
        stage('Code Quality Check Test'){
            steps{
                echo "Code quality check test"
            }
        }
        }
    }
}
